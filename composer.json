{
    "name": "linio/common-expressive",
    "description": "Common library for Linio projects using Zend Expressive",
    "keywords": ["linio", "common", "zend-expressive", "expressive"],
    "type": "library",
    "license": "BSD-3-Clause",
    "require": {
        "php": "^7.1",
        "psr/http-message": "^1.0.1",
        "linio/microlog": "^1.0",
        "linio/util": "^2.1",
        "particle/filter": "^1.4",
        "particle/validator": "^2.1",
        "monolog/monolog": "^1.20",
        "zendframework/zend-expressive": "^2.2",
        "zendframework/zend-expressive-router": "^2.4",
        "http-interop/http-middleware": "^0.4.1"
    },
    "require-dev": {
        "phpunit/phpunit": "^7.3",
        "eloquent/phony": "^3.1",
        "friendsofphp/php-cs-fixer": "^2.8"
    },
    "autoload": {
        "files": [
            "src/Support/functions.php"
        ],
        "psr-4": {
            "Linio\\Common\\Expressive\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Linio\\TestAssets\\": "tests/assets"
        }
    }
}
