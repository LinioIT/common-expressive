{
    "name": "linio/common-expressive",
    "description": "Common library for Linio projects using Zend Expressive",
    "keywords": ["linio", "common", "zend-expressive", "expressive"],
    "type": "library",
    "license": "BSD-3-Clause",
    "require": {
        "php": "^7.0",
        "psr/http-message": "^1.0",
        "linio/microlog": "^1.0",
        "linio/util": "^2.1",
        "particle/filter": "^1.4",
        "particle/validator": "^2.1",
        "monolog/monolog": "^1.20",
        "zendframework/zend-expressive": "^1.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^5.4",
        "eloquent/phony": "^0.13.0",
        "friendsofphp/php-cs-fixer": "^1.12-dev"
    },
    "autoload": {
        "files": [
            "src/Support/functions.php"
        ],
        "psr-4": {
            "Linio\\Common\\Expressive\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Linio\\TestAssets\\": "tests/assets"
        }
    },
    "config": {
        "bin-dir": "bin"
    }
}
